<div class="main-container">
        <div class="main-articles">
                {{!-- <h2>Articles</h2> --}}
                {{#each article}}
                <div class="articles">

                        <h3>{{title}}</h3>
                        <p>{{body}}</p>
                        <a href={{link}} target="_blank">{{link}}</a>
                        <div class="buttons">
                                <a href="#add-note" rel="modal:open"><button>Add A Note</button></a>
                        </div>
                </div>

                {{/each}}
        </div>
        <div class="notes"></div>
</div>